{% extends 'front_base.html.twig' %}

{% block body %}
<main class="bg-gray-300 antialiased">
    <h1 class="text-2xl font-bold text-center my-4">Your orders</h1>
    {{ dump(orders) }}
    {% for order in orders %}
        {% for orderDetails in order.orderDetails %}
            {{ dump(orderDetails) }}
        {% endfor %}
        {% for orderHistories in order.orderHistories %}
            {{ dump(orderHistories) }}
        {% endfor %}
    {% endfor %}

</main>
{% endblock %}